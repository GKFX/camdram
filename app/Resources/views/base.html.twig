	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());
		
		gtag('config', 'UA-37770738-2');
	</script>
	
	<meta charset="utf-8" />
        <meta http-equiv="x-ua-compatible" content="ie=edge" />

        <meta name="keywords" content="camdram,cambridge,theatre,drama,comedy,musicals,shows,society,societies,ACTS,association" />
        <meta name="description" content="Camdram is an information hub for the student performing arts community in Cambridge, England." />

        <meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=no" />
        
        {# Opengraph properties for facebook posts #}
        <meta property="og:type" content="website" />
		<meta property="og:locale" content="en_GB" />
		<meta property="og:site_name" content="Camdram" />
		<meta property="fb:app_id" content="464689303574287" />
        {% block opengraph %}{% endblock %}
        
        <!-- TODO link rel="apple-touch-icon" href="apple-touch-icon.png" -->
        <link rel='icon' href='/favicon.png' type='image/png' />

        <title>{% block title %}{% endblock %} - Camdram</title>

        <link rel="stylesheet" type="text/css" href="https://s3-eu-west-1.amazonaws.com/assets.cookieconsent.silktide.com/current/style.min.css"/>
        <link href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.css" rel="stylesheet"/>
        <link href="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/themes/ui-lightness/jquery-ui.css" rel="stylesheet"/>
        {% stylesheets output='css/main.css'
        'bundles/actscamdram/stylesheets/h5bp/normalize.css'
        'bundles/actscamdram/stylesheets/app/*.scss'
        'bundles/actsdiary/css/diary.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        {% block stylesheets %}{% endblock %}
        <!--[if lt IE 9]>
        <link href="{{ asset('bundles/actscamdram/stylesheets/app/ie8.css') }}" rel="stylesheet"/>
        <script src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.1.0/respond.min.js"></script>
        <![endif]-->
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
        {% javascripts output='js/deps.js'
        '@ActsCamdramBundle/Resources/public/javascripts/jquery/jquery-ui.touchpunch.js'
        '@ActsCamdramBundle/Resources/public/javascripts/foundation/custom.modernizr.js'
        '@ActsCamdramBundle/Resources/public/javascripts/foundation/foundation.min.js'
        '@ActsCamdramBundle/Resources/public/javascripts/moment.js'
        '@ActsCamdramBundle/Resources/public/javascripts/typeahead.js'
        '@ActsCamdramBundle/Resources/public/javascripts/dropzone.js'
        '@ActsDiaryBundle/Resources/public/js/diary.js'
        '@FOSJsRoutingBundle/Resources/public/js/router.js'
         %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% if app.environment == 'prod' %}
            <script src="{{ asset('js/fos_js_routes.js') }}"></script>
        {% else %}
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        {% endif %}
        {% javascripts output='js/main.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/camdram.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/autocomplete.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/home.js'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    <!-- Begin Cookie Consent plugin by Silktide - http://silktide.com/cookieconsent -->
