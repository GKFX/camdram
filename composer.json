{
	"name" : "camdram/camdram",
	"description" : "Camdram - The portal website for student theatre in Cambridge",
	"autoload" : {
		"psr-4" : {
			"" : "src/"
		},
		"classmap" : [
			"app/AppKernel.php",
			"app/AppCache.php"
		]
	},
	"license" : "GPL-2.0",
	"require" : {
		"php" : ">=5.6.0",
		"symfony/symfony" : "3.4.*",
		"doctrine/orm" : "^2.5",
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/doctrine-migrations-bundle" : "1.2.*",
		"doctrine/doctrine-fixtures-bundle" : "~2.2",
		"stof/doctrine-extensions-bundle" : "1.2.*",
		"twig/twig" : "^1.0||^2.0",
		"twig/extensions" : "~1.2",
		"symfony/assetic-bundle" : "~2.7",
		"symfony/swiftmailer-bundle" : "^2.6.4",
		"symfony/monolog-bundle" : "^3.1.0",
		"sensio/distribution-bundle" : "^5.0.19",
		"sensio/framework-extra-bundle" : "^5.0.0",
		"friendsofsymfony/rest-bundle" : "^2.3",
		"jms/serializer-bundle" : "^2.3",
		"friendsofsymfony/oauth-server-bundle" : "^1.5",
		"acts/social-api-bundle" : "0.1.*",
		"acts/external-login-bundle" : "0.2.*",
		"egeloen/google-map-bundle" : "^3.0",
		"pagerfanta/pagerfanta" : "1.0.*",
		"friendsofsymfony/jsrouting-bundle" : "1.6.*",
		"sabre/vobject" : "~3.3",
		"hoyes/symfony2-image-manager" : "0.1.*",
		"camdram/sphinx-realtime-bundle" : "0.1.*",
		"incenteev/composer-parameter-handler" : "^2.0",
		"acts/time-mock-bundle" : "*@dev",
		"zurb/foundation" : "4.*",
		"leafo/scssphp" : "0.6.*",
		"zendframework/zend-feed" : "2.4.*",
		"zendframework/zend-mail" : "2.4.*",
		"zendframework/zend-servicemanager" : "2.4.*",
		"excelwebzone/recaptcha-bundle" : "^1.5"
	},
	"require-dev" : {
		"sensio/generator-bundle" : "^3.0",
		"behat/behat" : "3.4.*",
		"behat/mink" : "^1.6",
		"behat/mink-extension" : "^2.3",
		"behat/mink-goutte-driver" : "*",
		"behat/symfony2-extension" : "^2.1",
		"symfony/phpunit-bridge": "^3.0"
	},
	"scripts" : {
		 "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
	},
	"minimum-stability" : "stable",
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-tests-dir": "tests",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml"
		},
		"symfony-assets-install" : "relative"
	},
	"config" : {
		"bin-dir" : "bin/",
		"platform": {
            "php": "5.6.0"
        },
        "sort-packages": true
	}
}