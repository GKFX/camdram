{
    "name": "camdram/camdram",
    "description": "Camdram - The portal website for student theatre in Cambridge",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "license" : "GPL-2.0",
    "require": {
        "php": ">7.0.0",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "2.5.*",
        "doctrine/doctrine-bundle": "~1.8",
        "doctrine/doctrine-migrations-bundle": "1.3.*",
        "doctrine/doctrine-fixtures-bundle": "~2.4",
        "stof/doctrine-extensions-bundle": "1.3.*",
        "twig/extensions": "~1.2",
        "symfony/assetic-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "~2.6",
        "symfony/monolog-bundle": "~3.2",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0",
        "friendsofsymfony/rest-bundle": "^2.3",
        "jms/serializer-bundle": "^2.3",
        "friendsofsymfony/oauth-server-bundle": "1.5.*",
        "sensio/buzz-bundle": "1.*",
        "egeloen/google-map-bundle" : "~2.2",
        "pagerfanta/pagerfanta": "1.0.*",
        "friendsofsymfony/jsrouting-bundle": "2.2.*",
        "sabre/vobject": "~3.3",
        "friendsofsymfony/elastica-bundle": "^4.1",
        "incenteev/composer-parameter-handler": "~2.1",
        "liip/imagine-bundle": "~1.9",
        "zurb/foundation": "4.*",
        "leafo/scssphp": "0.6.*",
        "zendframework/zend-feed": "2.4.*",
        "zendframework/zend-servicemanager": "2.4.*",
        "excelwebzone/recaptcha-bundle" : "2.6.*@dev",
        "hwi/oauth-bundle": "^0.6",
        "oneup/uploader-bundle": "^1.9",
        "facebook/graph-sdk": "^5.6",
        "abraham/twitteroauth": "^0.5.4",
        "knplabs/github-api": "^1.7",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.10"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "behat/behat": "2.5.*@stable",
        "behat/mink": "1.6.*@stable",
        "behat/mink-extension": "1.3.*",
        "behat/mink-goutte-driver": "1.1.0",
        "behat/symfony2-extension": "1.1.*",
        "phpunit/phpunit": "5.7.*"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-create-project-cmd": [
            "Acts\\CamdramAdminBundle\\Composer\\ScriptHandler::refreshDatabase",
            "Acts\\CamdramAdminBundle\\Composer\\ScriptHandler::welcomeMessage"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "symfony-assets-install": "relative"
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "7.0.30"
        }
    }
}
