<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActsAuthtokens" table="acts_authtokens">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="token" type="string" column="token" length="50" nullable="false"/>
    <field name="siteid" type="integer" column="siteid" nullable="false"/>
    <field name="userid" type="integer" column="userid" nullable="false"/>
    <field name="issued" type="datetime" column="issued" nullable="false"/>
  </entity>
</doctrine-mapping>
