<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActsEmailItems" table="acts_email_items">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="emailid" type="integer" column="emailid" nullable="false"/>
    <field name="title" type="text" column="title" nullable="false"/>
    <field name="text" type="text" column="text" nullable="false"/>
    <field name="orderid" type="float" column="orderid" nullable="false"/>
    <field name="creatorid" type="integer" column="creatorid" nullable="false"/>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="protect" type="boolean" column="protect" nullable="false"/>
  </entity>
</doctrine-mapping>
