<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActsPages" table="acts_pages">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="text" column="title" nullable="false"/>
    <field name="parentid" type="integer" column="parentid" nullable="false"/>
    <field name="sortcode" type="integer" column="sortcode" nullable="false"/>
    <field name="fulltitle" type="text" column="fulltitle" nullable="false"/>
    <field name="secure" type="boolean" column="secure" nullable="false"/>
    <field name="micro" type="boolean" column="micro" nullable="false"/>
    <field name="help" type="text" column="help" nullable="false"/>
    <field name="ghost" type="boolean" column="ghost" nullable="false"/>
    <field name="mode" type="string" column="mode" nullable="true"/>
    <field name="allowsubpage" type="integer" column="allowsubpage" nullable="false"/>
    <field name="intertitle" type="text" column="intertitle" nullable="false"/>
    <field name="knowledgebase" type="boolean" column="knowledgebase" nullable="false"/>
    <field name="getvars" type="text" column="getvars" nullable="false"/>
    <field name="postvars" type="text" column="postvars" nullable="false"/>
    <field name="usepage" type="text" column="usepage" nullable="false"/>
    <field name="kbid" type="integer" column="kbid" nullable="false"/>
    <field name="rssfeeds" type="text" column="rssfeeds" nullable="false"/>
    <field name="locked" type="boolean" column="locked" nullable="false"/>
    <field name="virtual" type="boolean" column="virtual" nullable="false"/>
    <field name="paramParser" type="boolean" column="param_parser" nullable="false"/>
    <field name="accessPhp" type="text" column="access_php" nullable="false"/>
    <field name="subpagetemplate" type="text" column="subpagetemplate" nullable="false"/>
    <field name="searchable" type="boolean" column="searchable" nullable="false"/>
  </entity>
</doctrine-mapping>
