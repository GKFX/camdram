    </id>
    <field name="rid" type="integer" column="rid" nullable="false"/>
    <field name="email" type="text" column="email" nullable="false"/>
    <field name="type" type="string" column="type" nullable="false"/>
    <field name="issuerid" type="integer" column="issuerid" nullable="false"/>
    <field name="creationdate" type="date" column="creationdate" nullable="false"/>
  </entity>
</doctrine-mapping>
